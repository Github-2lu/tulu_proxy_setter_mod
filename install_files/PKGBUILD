pkgname="tulu_proxy_setter"
pkgver=0.1.alpha
pkgrel=1
pkgdesc="Auto Proxy Setter"
arch=("x86_64")
url="https://github.com/Github-2lu/tulu_proxy_setter_mod"
license=("GPL3")
depends=('gtk3')
makedepends=("git")
source=("$pkgname.tar.gz::$url/releases/download/v$pkgver/$pkgname.tar.gz")
md5sums=("SKIP")

package() {
	cp -r $srcdir/* $pkgdir
}
